$("#schedule-modal").html("<%= escape_javascript(render 'edit') %>")
$(".datepicker").datetimepicker({
  ignoreReadonly: true,
  format: "YYYY/MM/DD"
})
$('#startat').datetimepicker({
  format: 'HH:mm',
  stepping: 5,
  ignoreReadonly: true,
  defaultDate: moment({ h: 11 })
})
$('#endat').datetimepicker({
  format: 'HH:mm',
  stepping: 5,
  ignoreReadonly: true,
  defaultDate: moment({ h: 15 })
})
$("#schedule-modal").modal("show")
